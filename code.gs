  const ss = getDb();
  let sheet = ss.getSheetByName('Problems');
  if (!sheet) { initialSetup(); sheet = ss.getSheetByName('Problems'); }

  const data = sheet.getDataRange().getValues();
  const problems = [];
  
  for (let i = 1; i < data.length; i++) {
    const row = data[i];
    if (row[0] === groupName && row[3] === '') {
      problems.push({
        q: row[1],
        a: row[2]
      });
    }
  }
  return problems;
}

function saveProblemSet(groupName, problems) {
  const ss = getDb();
  let sheet = ss.getSheetByName('Problems');
  if (!sheet) { initialSetup(); sheet = ss.getSheetByName('Problems'); }

  const data = sheet.getDataRange().getValues();
  const now = new Date();
  
  for (let i = 1; i < data.length; i++) {
    if (data[i][0] === groupName && data[i][3] === '') {
      sheet.getRange(i + 1, 4).setValue(now);
    }
  }
  
  const newRows = problems.map(p => [groupName, p.q, p.a, '']);
  if(newRows.length > 0) {
    sheet.getRange(sheet.getLastRow() + 1, 1, newRows.length, 4).setValues(newRows);
  }
  return { success: true };
}

function deleteProblemGroup(groupName) {
